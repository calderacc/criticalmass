<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <imports>
        <import resource="services/manager.xml" />
        <import resource="services/factory.xml" />
        <import resource="services/facebook.xml" />
    </imports>

    <services>
        <service id="caldera.criticalmass.facebookapi.eventride" class="AppBundle\Facebook\FacebookEventRideApi">
            <argument type="string">%facebook.app_id%</argument>
            <argument type="string">%facebook.app_secret%</argument>
            <argument type="string">%facebook.default_token%</argument>
        </service>

        <service id="caldera.criticalmass.facebookapi.citypageproperties" class="AppBundle\Facebook\FacebookPageApi">
            <argument type="string">%facebook.app_id%</argument>
            <argument type="string">%facebook.app_secret%</argument>
            <argument type="string">%facebook.default_token%</argument>
        </service>
    </services>
</container>