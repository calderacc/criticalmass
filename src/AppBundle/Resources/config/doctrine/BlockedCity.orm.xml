<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BlockedCity" repository-class="AppBundle\Repository\BlockedCityRepository" table="city_blocked">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="blockStart" type="datetime" nullable="true"/>
    <field name="blockEnd" type="datetime" nullable="true"/>
    <field name="description" type="text" nullable="true"/>
    <field name="url" type="string" length="255" nullable="true"/>
    <field name="facebook" type="string" length="255" nullable="true"/>
    <field name="twitter" type="string" length="255" nullable="true"/>
    <field name="photosLink" type="boolean" nullable="true"/>
    <field name="rideListLink" type="boolean" nullable="true"/>
    <many-to-one field="city" target-entity="AppBundle\Entity\City" fetch="LAZY">
      <join-columns>
        <join-column name="city_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
