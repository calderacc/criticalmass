<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BikerightVoucher" repository-class="AppBundle\Repository\BikerightVoucherRepository" table="bikeright_voucher">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="code" type="string" length="16" nullable="true"/>
    <field name="priority" type="smallint" nullable="true"/>
    <field name="createdAt" type="datetime" nullable="false"/>
    <field name="assignedAt" type="datetime" nullable="true"/>
    <many-to-one field="user" target-entity="AppBundle\Entity\User" inversed-by="bikerightVouchers">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
