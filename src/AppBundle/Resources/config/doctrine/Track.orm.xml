<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Track" repository-class="AppBundle\Repository\TrackRepository" table="track">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="username" type="string" length="255" nullable="true"/>
    <field name="creationDateTime" type="datetime" nullable="true"/>
    <field name="startDateTime" type="datetime" nullable="true"/>
    <field name="endDateTime" type="datetime" nullable="true"/>
    <field name="distance" type="float" nullable="true"/>
    <field name="points" type="integer" nullable="true"/>
    <field name="startPoint" type="integer" nullable="true"/>
    <field name="endPoint" type="integer" nullable="true"/>
    <field name="md5Hash" type="string" length="32" nullable="true"/>
    <field name="enabled" type="boolean" nullable="true"/>
    <field name="deleted" type="boolean" nullable="true"/>
    <field name="latLngList" type="text" nullable="true"/>
    <field name="geoJson" type="text" nullable="true"/>
    <field name="polyline" type="text" nullable="true"/>
    <field name="trackFilename" type="string" length="255" nullable="true"/>
    <field name="updatedAt" type="datetime" nullable="true"/>
    <field name="source" type="string" nullable="true" column-definition="ENUM('TRACK_SOURCE_GPX', 'TRACK_SOURCE_STRAVA', 'TRACK_SOURCE_RUNKEEPER', 'TRACK_SOURCE_RUNTASTIC', 'TRACK_SOURCE_DRAW', 'TRACK_SOURCE_GLYMPSE', 'TRACK_SOURCE_CRITICALMAPS', 'TRACK_SOURCE_UNKNOWN')"/>
    <field name="stravaActitityId" type="integer" nullable="true"/>
    <many-to-one field="ride" target-entity="AppBundle\Entity\Ride" inversed-by="tracks">
      <join-columns>
        <join-column name="ride_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="AppBundle\Entity\User" inversed-by="tracks">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
