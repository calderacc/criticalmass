<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Photo" repository-class="AppBundle\Repository\PhotoRepository" table="photo">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="latitude" type="float" nullable="true"/>
    <field name="longitude" type="float" nullable="true"/>
    <field name="description" type="text" nullable="true"/>
    <field name="views" type="integer" nullable="true"/>
    <field name="enabled" type="boolean" nullable="true"/>
    <field name="deleted" type="boolean" nullable="true"/>
    <field name="dateTime" type="datetime" nullable="true"/>
    <field name="creationDateTime" type="datetime" nullable="true"/>
    <field name="imageName" type="string" length="255" nullable="true"/>
    <field name="backupName" type="string" length="255" nullable="true"/>
    <field name="updatedAt" type="datetime" nullable="true"/>
    <many-to-one field="user" target-entity="AppBundle\Entity\User">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="ride" target-entity="AppBundle\Entity\Ride" inversed-by="photos">
      <join-columns>
        <join-column name="ride_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="city" target-entity="AppBundle\Entity\City" inversed-by="photos">
      <join-columns>
        <join-column name="city_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
