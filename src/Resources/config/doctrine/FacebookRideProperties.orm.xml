<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FacebookRideProperties" repository-class="AppBundle\Repository\FacebookRidePropertiesRepository" table="facebook_ride_properties">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" length="255" nullable="true"/>
    <field name="description" type="text" nullable="true"/>
    <field name="numberAttending" type="integer" nullable="true"/>
    <field name="numberMaybe" type="integer" nullable="true"/>
    <field name="numberDeclined" type="integer" nullable="true"/>
    <field name="numberInterested" type="integer" nullable="true"/>
    <field name="numberNoreply" type="integer" nullable="true"/>
    <field name="startTime" type="datetime" nullable="true"/>
    <field name="endTime" type="datetime" nullable="true"/>
    <field name="updatedTime" type="datetime" nullable="true"/>
    <field name="location" type="string" length="255" nullable="true"/>
    <field name="locationAddress" type="text" nullable="true"/>
    <field name="latitude" type="float" nullable="true"/>
    <field name="longitude" type="float" nullable="true"/>
    <field name="createdAt" type="datetime" nullable="true"/>
    <many-to-one field="ride" target-entity="AppBundle\Entity\Ride">
      <join-columns>
        <join-column name="ride_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
