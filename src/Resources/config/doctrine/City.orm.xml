<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\City" repository-class="AppBundle\Repository\CityRepository" table="city">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="city" type="string" length="50" nullable="true"/>
    <field name="title" type="string" length="100" nullable="true"/>
    <field name="description" type="text" nullable="true"/>
    <field name="url" type="string" length="255" nullable="true"/>
    <field name="facebook" type="string" length="255" nullable="true"/>
    <field name="twitter" type="string" length="255" nullable="true"/>
    <field name="latitude" type="float" nullable="true"/>
    <field name="longitude" type="float" nullable="true"/>
    <field name="enabled" type="boolean" nullable="true"/>
    <field name="cityPopulation" type="integer" nullable="true"/>
    <field name="punchLine" type="string" nullable="true"/>
    <field name="longDescription" type="text" nullable="true"/>
    <field name="imageName" type="string" length="255" nullable="true"/>
    <field name="updatedAt" type="datetime" nullable="true"/>
    <field name="createdAt" type="datetime" nullable="true"/>
    <field name="enableBoard" type="boolean" nullable="true"/>
    <field name="timezone" type="string" length="255" nullable="true"/>
    <field name="threadNumber" type="integer" nullable="true"/>
    <field name="postNumber" type="integer" nullable="true"/>
    <field name="colorRed" type="integer" nullable="true"/>
    <field name="colorGreen" type="integer" nullable="true"/>
    <field name="colorBlue" type="integer" nullable="true"/>
    <field name="views" type="integer" nullable="true"/>
    <many-to-one field="user" target-entity="AppBundle\Entity\User">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="region" target-entity="AppBundle\Entity\Region" inversed-by="cities">
      <join-columns>
        <join-column name="region_id" referenced-column-name="id"/>
      </join-columns>
      <cascade>
        <cascade-persist/>
      </cascade>
    </many-to-one>
    <many-to-one field="mainSlug" target-entity="AppBundle\Entity\CitySlug">
      <join-columns>
        <join-column name="main_slug_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="lastThread" target-entity="AppBundle\Entity\Thread">
      <join-columns>
        <join-column name="lastthread_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <one-to-many field="cycles" target-entity="AppBundle\Entity\CityCycle" mapped-by="city">
      <cascade>
        <cascade-persist/>
        <cascade-remove/>
      </cascade>
    </one-to-many>
    <one-to-many field="slugs" target-entity="AppBundle\Entity\CitySlug" mapped-by="city">
      <cascade>
        <cascade-persist/>
        <cascade-remove/>
      </cascade>
    </one-to-many>
    <one-to-many field="photos" target-entity="AppBundle\Entity\Photo" mapped-by="city"/>
    <one-to-many field="posts" target-entity="AppBundle\Entity\Post" mapped-by="city"/>
    <one-to-many field="rides" target-entity="AppBundle\Entity\Ride" mapped-by="city"/>
  </entity>
</doctrine-mapping>
