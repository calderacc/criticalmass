<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FacebookCityProperties" repository-class="AppBundle\Repository\FacebookCityPropertiesRepository" table="facebook_city_properties">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" length="255" nullable="true"/>
    <field name="about" type="text" nullable="true"/>
    <field name="description" type="text" nullable="true"/>
    <field name="generalInfo" type="text" nullable="true"/>
    <field name="likeNumber" type="integer" nullable="true"/>
    <field name="checkinNumber" type="integer" nullable="true"/>
    <field name="website" type="string" length="255" nullable="true"/>
    <field name="createdAt" type="datetime" nullable="true"/>
    <many-to-one field="city" target-entity="AppBundle\Entity\City">
      <join-columns>
        <join-column name="city_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
