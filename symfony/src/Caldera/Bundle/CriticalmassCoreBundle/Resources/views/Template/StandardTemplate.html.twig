{% extends 'CalderaCriticalmassCoreBundle:Template:MasterTemplate.html.twig' %}

{% block masterContent %}
    {% include('CalderaCriticalmassCoreBundle:Template:Includes/_navigation.html.twig') with { 'fullscreenMap': 'false' } %}

    {% block breadcrumbContainer %}
        <div class="container">
            <ol class="breadcrumb">
                <li>
                    <a href="{{ path('caldera_criticalmass_frontpage') }}">Critical Mass</a>
                </li>
                {% block breadcrumb %}{% endblock %}
            </ol>
        </div>
    {% endblock %}

    {% block content %}{% endblock %}

    {% include('CalderaCriticalmassCoreBundle:Template:Includes/_adserver.html.twig') %}

    {% include('CalderaCriticalmassCoreBundle:Template:Includes/_footer.html.twig') %}

    {% include('CalderaCriticalmassCoreBundle:Template:Includes/_piwik.html.twig') %}

    <script>
        CriticalMass.loadModule('Notification');
    </script>
{% endblock %}
